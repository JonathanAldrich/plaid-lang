package plaid.demo.plugin;

import java.util.ArrayList;

state Dead case of Liveness {
    
    /*
     * Toggles this cell in place.
     */
    method toggle() {
        this <- Alive;
    }
   
    
    /*
     * @returns the Color of the cell
     */
    method getColor() {
        java.awt.Color.GRAY
    }
}