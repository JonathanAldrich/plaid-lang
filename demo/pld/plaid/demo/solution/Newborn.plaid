package plaid.demo.solution;

import java.util.ArrayList;

state Newborn case of Liveness {

    /*
     * Toggles this cell in place.
     */
    method toggle() {
        this <- Dead;
    }
    
    /*
     * @returns the Color of the cell
     */
    method getColor() {
        java.awt.Color.GREEN;
    }
}