package testInputs.fibonacci;

method immutable Integer fibonacci(immutable Integer n) {
	ifElse (n <= 2) {
		1;
	} {
		fibonacci(n - 1) + fibonacci(n - 2);
	};
}

method unit main() {
}

val (immutable Integer) -> immutable Integer fib = fn (immutable Integer n) => {
	match (n <= 2) {
		case True {
			1;
		}
		case False {
			fib(n-1) + fib(n-2);
		}
	}
}