package plaid.ast;

// Split is supposed to be used in a let binding-like fashion, i.e.:
// Split (in : PermType => out1 : PermType / out2 : PermType) in EXPR

state Split case of Expression {
	val /*ID*/ in;
	val /*ID*/ out1;
	val /*ID*/ out2;
	
	val /*Expression*/ body;
	
	method accept(/*ASTVisitor*/ v) {
		v.visitSplit(this);
	}
}