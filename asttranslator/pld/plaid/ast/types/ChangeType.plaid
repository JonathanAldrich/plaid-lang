package plaid.ast.types;

state ChangeType {
	
	val /*PermType*/ input;
	val /*PermType*/ output;
	val /*Boolean*/ borrow;
	
	method ==(/*PermTypeChange*/ other) {
		this.from == other.from && this.to == other.to;
	}
	
	method toString() {
		input.toString() + " " + ">>" + " " + output.toString()
	}
}