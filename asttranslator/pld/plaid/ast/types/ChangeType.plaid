package plaid.ast.types;

state ChangeType {
	
	val /*PermType*/ input;
	val /*PermType*/ output;
	val /*Boolean*/ borrow;
	
	method ==(/*PermTypeChange*/ other) {
		this.input == other.input && this.output == other.output && (this.borrow == other.borrow);
	}
	
	method toString() {
		var ret = input.toString() + " ";
		ifElse ( this.borrow ) 
		 { ret = ret + "><" }
		 { ret = ret + ">>" };
		 ret = ret +  " " + output.toString();
		 ret
	}
}