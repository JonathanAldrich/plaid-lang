package plaid.ast.types;

state NonePermission case of Permission {
	method isWritable() {
		false;
	}

	method split(p2) {
		match(p2) {
			// none => none/none
			case NonePermission {
				new Some{ val value = (p2, this); };
			}
			default {
				new None;
			}
		};
	}
	
	method ==(p2) {
		match(p2) {
			case NonePermission {
				true;
			}
			default {
				false;
			}
		};
	}
	
	method merge(p2) {
		p2;
	}
	
	method toString() {
		"none"
	}
}