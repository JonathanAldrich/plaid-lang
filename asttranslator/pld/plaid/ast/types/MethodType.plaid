package plaid.ast.types;

state MethodType case of Type {
	val /*String*/ name;
	val /*Type*/ returnType;
	val /*Type*/ argType;
	val /*PermTypeChange*/ receiver;

	method ==(other) {
		match (other) {
			case MethodType {
				this.returnType == other.returnType
				&& this.argType == other.argType
				&& this.receiver == other.receiver;
			}
			default {
				false;
			}
		};
	}
}