package plaid.ast.types;

state NominalType {
	val /*ID*/ typeName;
	
	method ==(/*NominalType*/ other) {
		match (other) {
			case NominalType {
				this.typeName.equals(other.typeName); //TODO: will this convert to Plaid boolean?
			}
			default {
				false;
			}
		
		}
	
	}
	
	method toString() {
		"N(" + this.typeName.name + ")"
	}
	

}