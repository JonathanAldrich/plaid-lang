package plaid.ast;

state StateDecl case of ASTNode {
	val /*Token*/ token;
	val /*ID*/ name;
	val /*State*/ stateDef;
	val /*Option QI*/ caseOf;
	val /*boolean*/ isCaseOf;
	val /*StateType*/ stateType = new UncheckedType; //TODO: fix me;

	method accept(v) {
		v.visitStateDecl(this)
	}
	
	method getName() {
		this.name.name
	}
}