package plaid.ast;

state ImportList case of ASTNode {
	val /*List<QualifiedID>*/ imports;
	val /*Token*/ token;
	
	method accept(v) {
		v.visitImportList(this)
	}
	
	method copy() {
		new ImportList {
			val /*List<QualifiedID>*/ imports = this.imports.map(fn (qi) => qi.copy());
			val /*Token*/ token = this.token;
		}
	}
}