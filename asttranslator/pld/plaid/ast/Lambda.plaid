package plaid.ast;

state Lambda case of Expression {
	val /*Token*/ token;
	val /*Option ID*/ x;
	val /*Expression*/ body;
	val /*LambdaType*/ lambdaType;

	method accept(v) {
		v.visitLambda(this)
	}
	
	method copy() {
		new Lambda {
			val /*Token*/ token = this.token;
			val /*Option ID*/ x = this.x.copy();
			val /*Expression*/ body = this.body.copy();
			val /*LambdaType*/ lambdaType = this.lambdaType;
		}
	}
}