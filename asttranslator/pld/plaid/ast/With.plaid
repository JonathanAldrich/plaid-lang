package plaid.ast;

state With case of ASTNode {
	val /*Token*/ token;
	val /*State*/ s1;
	val /*State*/ s2;

	method accept(v) {
		v.visitWith(this)
	}
	
	method copy() {
		new With {
			val /*Token*/ token = this.token;
			val /*State*/ s1 = this.s1.copy();
			val /*State*/ s2 = this.s1.copy();
		}
	}	
}