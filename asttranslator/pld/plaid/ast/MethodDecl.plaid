package plaid.ast;

state MethodDecl case of ASTNode {
	val /*Token*/ token;
	val /*String*/ name;
	val /*Expression*/ body;
	val /*Option ID*/ arg;
	val /*boolean*/ abstractMethod;
	val /*MethodType*/ methodType;

	method accept(v) {
		v.visitMethodDecl(this)
	}
	
	method getName() {
		this.name
	}
}