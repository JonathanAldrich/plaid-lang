package plaid.HashtableBenchmark;

import plaid.examples.lib.hashtable.Hashtable;
import plaid.examples.lib.hashtable.global.makeGlobalHashtable;

method void runGlobalSharedBenchmark() {
    // create hashtable 
    val unique Hashtable ht = makeGlobalHashtable(6);
    
    // run benchmark
    runSharedBenchmark(ht, 16, 8);
}