package plaid.typechecker.ast;

import plaid.ast.Expression;

state LNFChangeState case of Expression {
	val /*Token*/ token;
	val /*ID*/ e;
	val /*State*/ s;
	
	method accept(v) {
		v.visitChangeState(this)
	}
	
	method copy() {
		new LNFChangeState {
			val /*Token*/ token = this.token;
			val /*ID*/ e = this.e.copy();
			val /*State*/ s = this.s.copy();
		}
	}	
}