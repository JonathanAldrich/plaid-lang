package plaid.typechecker.ast;

import plaid.ast.Expression;

state LNFDestructiveDereference case of Expression {
	val /*Token*/ token;
	val /*ID*/ left;
	val /*ID*/ right;

	method accept(v) {
		v.visitDestructiveDereference(this)
	}
	
	method copy() {
		new LNFDereference {
			val /*Token*/ token = this.token;
			val /*ID*/ left = this.left.copy();
			val /*ID*/ right = this.right.copy();
		}
	}
}