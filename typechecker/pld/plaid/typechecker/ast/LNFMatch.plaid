package plaid.typechecker.ast;

import plaid.ast.Expression;

state LNFMatch case of Expression {
	val /*Token*/ token;
	val /*ID*/ e;
	val /*List<Case>*/ caseList;

	method accept(v) {
		v.visitMatch(this)
	}
	
	method copy() {
		new LNFMatch {
			val /*Token*/ token = this.token;
			val /*ID*/ e = this.e.copy();
			val /*List<Case>*/ caseList = this.caseList.map(fn (c) => c.copy());
		}
	}	
}