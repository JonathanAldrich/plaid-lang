package plaid.typechecker.ast;

import plaid.ast.Expression;

state LNFApplication case of Expression {
	val /*Token*/ token;
	val /*ID*/ f;
	val /*ID*/ arg;
	
	method accept(v) {
		v.visitApplication(this)
	}
	
	method copy() {
		new LNFApplication {
			val /*Token*/ token = this.token;
			val /*ID*/ f = this.f.copy();
			val /*ID*/ arg = this.arg.copy();
		}
	}
}