package plaid.typechecker.ast;

import plaid.ast.Application;

state LNFApplication case of Application {
	override val /*Token*/ token;
	override val /*ID*/ f;
	override val /*ID*/ arg;
	
	override method accept(v) {
		v.visitApplication(this)
	}
	
	override method copy() {
		new LNFApplication {
			val /*Token*/ token = this.token;
			val /*ID*/ f = this.f.copy();
			val /*ID*/ arg = this.arg.copy();
		}
	}
}