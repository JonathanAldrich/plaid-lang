package plaid.typechecker;

state StateTable {
	val /*Map<ID,StateType>*/ states = HashMap.new();
	
	method put(/*ID*/ name, /*StateType*/ type) {
		states.put(name,type);
	}
	
	method /*StateType*/ get(/*ID*/ name) {
		states.get(name);
	}
	
	method printTable() {
        java.lang.System.out.println("test");
        val iter = states.keySet().iterator();
        while { iter.hasNext(); }{
            java.lang.System.out.print(states.get(iter.next()).toString());
        };
	}
	
	
}