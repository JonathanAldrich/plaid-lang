package plaid.typechecker.lnf.tests;

import plaid.ast.*;

val () -> void appTests = fn() => {
	val args = makeEmptyList();
	args.add(makeID("y"));
	args.add(makeID("z"));
	val unique Expression appBare = makeApplication(makeID("x"), args);
	rewrite(appBare);
	
	
	val args2 = makeEmptyList();
	args2.add(makeDereference(makeID("a"), makeID("b")));
	args2.add(makeDereference(makeID("c"), makeID("d")));
	val f2 = makeDereference(makeID("x"), makeID("y"));
	val unique Expression appComplex = makeApplication(f2, args2);
	rewrite(appComplex);
};