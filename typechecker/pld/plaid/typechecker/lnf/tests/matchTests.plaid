package plaid.typechecker.lnf.tests;

import plaid.ast.*;

val () -> void matchTests = fn() => {
	val cases = makeEmptyList();
	cases.add(makeCase(makeID("y")));
	cases.add(makeCase(makeID("z")));
	val m = makeMatch(makeID("x"), cases);
	
	rewrite(m);
	
	
	val cases2 = makeEmptyList();
	cases2.add(makeCase(makeID("y")));
	cases2.add(makeCase(makeID("z")));
	val m2 = makeMatch(makeDereference(makeID("a"), makeID("b")), cases2);
	
	rewrite(m2);
};