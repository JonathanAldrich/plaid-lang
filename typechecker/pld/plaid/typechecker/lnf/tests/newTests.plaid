package plaid.typechecker.lnf.tests;

import plaid.ast.*;

val () -> void newTests = fn() => {
	val unique NewInstance n = makeNewInstance(makeQI("N", new None));
	rewrite(n);
	
	val decls = makeEmptyList();
	decls.add(makeFieldDecl(makeID("f"), makeDereference(makeID("a"), makeID("b"))));
	decls.add(makeFieldDecl(makeID("g"), makeDereference(makeID("c"), makeID("d"))));
	val initState = makeDeclList(decls);
	val unique NewInstance complex = makeNewInstance(makeQI("N", createSome(initState)));
	rewrite(complex);
};