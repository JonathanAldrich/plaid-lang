package plaid.typechecker.lnf.tests;

import plaid.ast.*;

val () -> void letBindingTests = fn() => {
	val unique Expression letBare = makeLetBinding(makeID("x"), makeID("y"), makeID("z"));
	rewrite(letBare);
	
	val unique Expression letRead1 = makeLetBinding(makeID("x"), makeDereference(makeID("y"), makeID("f")), makeID("z"));
	rewrite(letRead1);
	
	val unique Expression letRead2 = makeLetBinding(makeID("x"), makeDereference(makeDereference(makeID("a"), makeID("b")), makeID("c")), makeID("z"));
	rewrite(letRead2);
};