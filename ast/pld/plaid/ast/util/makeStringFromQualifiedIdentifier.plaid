package plaid.ast.util;

method immutable String makeStringFromQualifiedIdentifier(immutable QualifiedIdentifier qi) {
    var name = "";
    
    if ( qi.identifiers.length() > 0 ) {
        name = qi.identifiers.get(0).name;
        var i = 1;
        while { i < qi.identifiers.length(); } {
            name = name + "." + qi.identifiers.get(i).name; 
            i = i + 1;
        };
    };
    
    name;    
}