package plaid.ast.parsed;

state Token {
    val /*Integer*/ beginLine;
    val /*Integer*/ beginColumn;
    val /*Integer*/ endLine;
    val /*Integer*/ endColumn;
    val /*String*/  image; 
    
    method /*String*/ errorOutput() {
    	"(line: " + beginLine + ",Column: " + beginColumn + ")";
    }
}