package plaid.fastexamples.javaInterop;

import java.lang.System;
import java.lang.StringBuilder;

method JavaNullaryConstructor() {

	val s = StringBuilder.new();
	match (s.toString() == "") {
		case True { System.out.println("nullary constructor succeeded!"); }
		case False { System.out.println("oops"); }
	};
	
}