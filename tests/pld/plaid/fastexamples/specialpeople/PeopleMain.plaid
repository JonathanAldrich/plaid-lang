package plaid.fastexamples.specialpeople;

import java.lang.System;

method PeopleMain() {
    val sven = new Person{val name="Sven"; val attribute="annoying";};
    val karl = new Person{val name="Karl"; val attribute="tall";};
    val josh = new Person{val name="Josh"; val attribute="brilliant";};
    
    System.out.println(sven.summary());
    System.out.println(karl.summary());
    System.out.println(josh.summary());
}