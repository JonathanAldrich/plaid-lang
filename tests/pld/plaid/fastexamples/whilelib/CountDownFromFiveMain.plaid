package plaid.fastexamples.whilelib;

import java.lang.System;

method CountDownFromFiveMain() {
	var counter = new MutableInt;
	counter.value = 5;
	while ( fn () => {counter.value >= 1}, 
			fn () =>{ 
				System.out.println(counter.value.toString());
				counter.value = counter.value - 1;
			});
}