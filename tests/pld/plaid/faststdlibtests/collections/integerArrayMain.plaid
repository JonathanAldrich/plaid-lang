package plaid.faststdlibtests.collections;

import plaid.collections.makeArray;

method integerArrayMain() {
	val theArray = makeArray(3);
	
	theArray.set(0,4);
	theArray.set(1,1);
	theArray.set(2,7);
	
	printLine(theArray.get(0).toString());
	printLine(theArray.get(1).toString());
	printLine(theArray.get(2).toString());
	
	theArray.set(1,5);
	
	printLine(theArray.get(1).toString());
}