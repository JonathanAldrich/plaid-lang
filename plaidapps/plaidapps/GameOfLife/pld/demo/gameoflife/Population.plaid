package demo.gameoflife;

state Population{}

state Populated case of Population{
  method onPopulated() {
	  this <- AliveCell;
	}
}

state Survive case of Population {
  method onSurvive() {
    this <- AliveCell;
	}
}

state Lonely case of Population {
  method onLonely() {
    this <- DeadCell;
	}
}

state OverPopulated case of Population {
  method onOverPopulated() {
	  this <- DeadCell;
	}
}
state Unpopulated case of Population {
  method onUnpopulated() {
	  this <- DeadCell;
	}
}
