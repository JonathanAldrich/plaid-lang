package plaid.examples.simpleFloatingTask;

state SimpleFloatingTask {
    
    @cheap
    method void LOG(immutable String msg) [ immutable SimpleFloatingTask this ] { }
    
    method void nop(unique SimpleFloatingTask obj) [ immutable SimpleFloatingTask this ] { }
    
    method void main(unique SimpleFloatingTask obj) [ immutable SimpleFloatingTask this ] {
        this.LOG("Hello World");
        this.nop(obj);    
    }
}