package plaid.examples.simpleSharedMatch;

state SimpleSharedMatch<group Owner> {
    var immutable Integer f = 0;
    
    method void simpleSharedMatch<group exclusive owner, group exclusive other>(immutable Boolean cond) [ shared<owner> SimpleSharedMatch<owner> this ] {
        match (cond) {
            case True { this.f = 1 }
            default   { this.f = 0 }
        }
    }

}