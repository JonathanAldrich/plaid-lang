package plaid.examples.lib.hashtable;

state Hashtable {
    method void addShared(immutable Hashable obj) [ local shared Hashtable this ];
    method void addUnique(immutable Hashable obj) [ unique Hashtable this ];
    method immutable Boolean containsShared(immutable Hashable obj)  [ local shared Hashtable this ];
    method immutable Boolean containsUnique(immutable Hashable obj)  [ unique Hashtable this ];
}