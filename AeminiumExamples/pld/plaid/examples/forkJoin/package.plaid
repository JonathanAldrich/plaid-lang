package plaid.examples.forkJoin;

method void forkJoin(immutable Integer level) {
    val immutable Boolean isZero = level == 0;
    
    match ( isZero ) {
        case False {
            val immutable Integer currentLevel = level - 1;
            forkJoin(currentLevel);
            forkJoin(currentLevel);
        }
        default { unit }
    }
} 


method void main() {
    forkJoin(29);
}