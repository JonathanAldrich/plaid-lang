package coreExamples.doubleTest;

import java.lang.*;

method main() {
    var shared = 0;
    val negative = -1;
    val pi = -3.14 ;
    val sixteen = 0x10;
    var mutable = 0;

    printLine("This should be zero: " + shared);
    printLine("This should be negative one: " + negative);
    printLine("This should be negative pi: " + pi);
    printLine("This should be sixteen: " + sixteen);
    printLine("This should be zero: " + mutable);
}
