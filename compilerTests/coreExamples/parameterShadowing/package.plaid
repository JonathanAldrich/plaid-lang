package coreExamples.parameterShadowing;

state S {
	var foo = 10;
	
	method bar(foo) {
		foo = 3;
		java.lang.System.out.println("This should be 3: " + foo);
	}
}

state T {
	method foo() {
		23
	}
	
	method bar(foo) {
		java.lang.System.out.println(foo);
	}
}

method main() {
	var s = new S;
	s.bar(5);
	java.lang.System.out.println("This should be 10: " + s.foo);

	java.lang.System.out.print("This should be 5: ");
	(new T).bar(5);
}