package coreExamples.referenceStateField;

state Test {
	var foo = "foo";
	
	method bar() {
		java.lang.System.out.println(foo);
		java.lang.System.out.println(this.foo);
		
		foo = "blah";
		this.foo = "blah";
	}
}

method main() {
	java.lang.System.out.println("This should print \"foo\" twice: (revisit adding \'this\' to field derefs)");
	var t = new Test;
	t.bar();
}