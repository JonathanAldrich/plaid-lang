
package coreExamples.boolTests;

import plaid.lang.True;
import plaid.lang.False;
import java.lang.System;

method main() {
	val x = true;
	System.out.print("This should say \"tags generated\": ");
	match (x) {
		case True { System.out.println("tags generated") }
		case False { System.out.println("Something strange happened") }
		default { System.out.println("Tags not generated and checked correctly") }
	}
}