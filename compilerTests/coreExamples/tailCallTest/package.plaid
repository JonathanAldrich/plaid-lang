package coreExamples.tailCallTest;

method sum(x) {
	var i = 1;
	while {i <= x} {
		i = i + 1;
	};
	i;
}

method main() {
	val begin = java.lang.System.currentTimeMillis();
	
	java.lang.System.out.println(sum(100000));
	
	val end = java.lang.System.currentTimeMillis();
	
	java.lang.System.out.println(begin);
	java.lang.System.out.println(end);
} 
 
