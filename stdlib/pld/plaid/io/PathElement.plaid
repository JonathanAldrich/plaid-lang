package plaid.io;

state PathElement
{
	val fileReference; //public private stuff? get,set methods?
	val name;
	val fullPath;
	val parentDir;
	val size;
	
	method immutable String getName()
	{
		name;
	}
	
	method immutable String getFullPath()
	{
		fullPath;
	}
	
	method immutable String getParentDir()
	{
		parentDir;
	}
	
	method immutable Integer getSize()
	{
		size.intValue(); // converts Long to Plaid's Integer type
	}
}