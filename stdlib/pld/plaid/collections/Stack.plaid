package plaid.collections;

/**
 * A really bad stack implementation.  Some of these methods should actually be eliminated.
 */
 
state Stack case of Collection = {
	var list = makeEmptyList();
	
	method add(elem){
		list.add(elem)
	}
	
	method size(){
		list.size()
	}
	
	method get(index){
		list.get(index)
	}
	
	method pop(){
		var ret = list.head.fst;
		list = list.head.snd;
		ret;
	}
	
	method filterAll(f){
		list.filterAll(f);
	}
}
