package examples.aeminium;

state MatchTest {
     method void matches(immutable Boolean value) [immutable MatchTest this] {
         val immutable Integer x = 1;
         val immutable Integer y = 2;
         match (value) {
             case True  {
                val immutable Integer  z = x  + 1;
                match (z) {
                    case Integer { x } 
                    default { y }
                }; 
             }
             case False { this }
         }
     }
}