package examples.aeminium;

state Assignment {
    var immutable Integer x = 1;
    var immutable String name = "Dereference";

    method unique Assignment makeAssignment() {
        val unique Assignment value = new Assignment;
        
        value.x = 666;
        value.name ="devil";
        
        value 
    }    

    method void assign() [unique Assignment this] { 
        this.x = 0;
        this.name = "Hello World!"
    } 
}