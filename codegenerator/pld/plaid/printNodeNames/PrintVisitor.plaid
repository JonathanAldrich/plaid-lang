	method visitChangeState(node) {
		if (debug) {
			printLine("//I am a ChangeState");
			printLine("   //e: " + node.e.toString());
			printLine("   //s: " + node.s.toString());
		};
		var toChange=node.e.accept(this);
		
		match(node.s){
			case QI{
				outputsArray.get(currentOutput).append(toChange+".stateChange("+node.s.accept(this)+");"+newLine);
			}
			case With{
				outputsArray.get(currentOutput).append(toChange+".stateChange("+node.s.accept(this)+");"+newLine);
			}
			case DeclList{
				node.s.decls.map(fn (d) => {
					match(d) {
						case MethodDecl{
							//d.accept(this);
							outputsArray.get(currentOutput).append(toChange+".stateChange("+toChange+".withMember('"+d.name+"', '"+d.accept(this)+"'));"+newLine);
							}
						case FieldDecl{			
							match(d.e){
								case UnitLiteral{
									//outputsArray.get(currentOutput).append(toChange+".stateChange("+toChange+".withMemberNoValue('"+d.f.name+"'));"+newLine);
									outputsArray.get(currentOutput).append(toChange+".stateChangeMemberNoValue('"+d.f.name+"');"+newLine);
								}
								default {
									//outputsArray.get(currentOutput).append(toChange+".stateChange("+toChange+".withMember('"+d.f.name+"', '"+d.e.accept(this)+"'));"+newLine);
									outputsArray.get(currentOutput).append(toChange+".stateChangeMember('"+d.f.name+"', '"+d.e.accept(this)+"');"+newLine);
								}
							};
						
						}
						default{
							d.accept(this);
						}
					};
				});
			}
			default{
				outputsArray.get(currentOutput).append("problem at changeState's s"+newLine);
				node.s.accept(this);
			}
		};
		toChange;
	}
	method visitCompilationUnit(node) {
		if (debug) {
			printLine("//I am a CompilationUnit");	
		};
		
		outputsArray.add(outputLines);
		outputsArray.add(stateOutputLines);
		
		var directoryFile = new plaid.io.FileObj;
		directoryFile.setName(targetDirectory);
		var modifiedPackage="";
		
		ifElse(directoryFile.canWrite()){
			var topDirectory="";
			node.packageName.map(fn (d) => {
				modifiedPackage=modifiedPackage+"/"+d;
				topDirectory=topDirectory+"/..";
			});
			directoryFile.setName(targetDirectory+modifiedPackage);
			directoryFile.mkdirs();
			topDirectory=topDirectory.replaceFirst("/..","..");
			directoryAbovePackage=topDirectory;
		}
		{
			printLine("Error: cannot write to the directory "+targetDirectory+modifiedPackage);
		};
		
		//create the array of strings that will be written to the file
		node.imports.accept(this);
		node.decls.map(fn (d) => {
			d.accept(this);
		});			
		
		var fileName=node.sourceFile;	
		var fileExtension=".js";
		if(browser==true && mainFile==true){
			fileExtension=".html";
		};
		fileName=fileName.replace(".plaid",fileExtension);
		output.setName(targetDirectory+modifiedPackage+"/"+fileName);
		output.beginWrite();
		
		ifElse (browser==true) {
		    if(mainFile==true){
				output.writeLine("<html>");
				output.writeLine("<head>");
				output.writeLine("<script src=\""+directoryAbovePackage+"/Tree.js\" type=\"text/javascript\"></script>");
				output.writeLine("<script src=\""+directoryAbovePackage+"/PlaidObject.js\" type=\"text/javascript\"></script>");
				output.writeLine("<script src=\""+directoryAbovePackage+"/PlaidState.js\" type=\"text/javascript\"></script>");
				output.writeLine("<script src=\""+directoryAbovePackage+"/Globals.js\" type=\"text/javascript\"></script>");
				output.writeLine("</head>");
				output.writeLine("<body>");
				output.writeLine("<script type=\"text/javascript\">");
				output.writeLine("<!--");
			
			
				output.writeLine("function include(filename){");
				output.writeLine("	fileType=\"text/javascript\";");
  				output.writeLine("	document.write(\"<script src=\"+filename+\" type=\"+fileType+\"></script>\");");
 				output.writeLine("}");
			};
		}
		{
			output.writeLine("load(\"PlaidObject.js\");");
			output.writeLine("load(\"PlaidState.js\");");
			output.writeLine("load(\"Tree.js\");");
			output.writeLine("load(\"Globals.js\");");
		};
		

		output.write(outputsArray.get(1).toString());
		output.write(outputsArray.get(0).toString());
		
		if (browser==true && mainFile==true){
			output.writeLine("//-->");
			output.writeLine("</script>");
			output.writeLine("</head>");
			output.writeLine("</body>");
		};
		output.endWrite();
		printLine("Done");
	}
	method visitDeclList(node) {
		if (debug) {
			printLine("//I am a DeclList");
		};
		//node.decls.map(fn (d) => {d.accept(this);});
		
		var nMinusOneDecls = node.decls.subList(0,node.decls.length() - 2);
		nMinusOneDecls.map(fn (d) => {d.accept(this);});
		node.decls.get(node.decls.length() - 1).accept(this);
	}
