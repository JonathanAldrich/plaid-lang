	method visitWith(node) {
		if (debug) {
			output.writeLine("//I am a With");
			output.writeLine ("   //s1: " + node.s1.toString());			
			output.writeLine ("   //s2: " + node.s2.toString());
		};
		var returnString="";
		var tagName="";
		match (node.s1) {
			case QI{
				tagName = node.s1.qid.toString();
				returnString=node.s1.accept(this);
			}
			case DeclList{
				tagName = generateNewVariableName();
				//output.writeLine("var plaidNewState_"+tagName+" = new PlaidState();");
				//returnString = generateNewStateName(tagName);
				returnString = "(new PlaidState())";
				node.s1.decls.map(fn (d) => {
					var newMember = d.accept(this);
					returnString=returnString+".withMember('"+newMember+"', "+newMember+")";
				});
			}
			default{
				output.writeLine("problem at With");
			}
		};
		match (node.s2) {
			case QI{
				returnString = returnString+".with("+node.s2.accept(this)+")"; 
			}
			case DeclList{
				node.s2.decls.map(fn (d) => {
					var newMember = d.accept(this);
					//returnString=returnString+".specialize('"+tagName+"', '"+newMember+"', "+newMember+")";
					returnString=returnString+".withMember('"+newMember+"', "+newMember+")";
				});
			}
			case With{
				returnString = returnString+withHelper(node.s2);
			}
			default{
				output.writeLine("problem at With");
				node.s2.accept(this);
			}
		};
		returnString;
	}
	method withHelper(node){
		var returnString="";
		match (node.s1) {
			case QI{
				returnString = ".with("+node.s1.accept(this)+")"; 
			}
			case DeclList{
				node.s1.decls.map(fn (d) => {
					var newMember = d.accept(this);
					returnString=returnString+".withMember('"+newMember+"', "+newMember+")";
				});
			}
			default{
				output.writeLine("problem at With");
				node.s1.accept(this);
			}
		};
		match (node.s2) {
			case QI{
				returnString = returnString+".with("+node.s2.accept(this)+")"; 
			}
			case DeclList{
				node.s2.decls.map(fn (d) => {
					var newMember = d.accept(this);
					returnString=returnString+".withMember('"+newMember+"', "+newMember+")";
				});
			}
			case With{
				returnString = returnString+this.withHelper(node.s2);
			}
			default{
				output.writeLine("problem at With");
				node.s2.accept(this);
			}
		};
		returnString;
	}

