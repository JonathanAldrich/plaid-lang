	method checkUniqueTags(root1,root2){
		var originalTags = root1.tags();
		var withTags = root2.tags();
		checkUniqueTagsHelper(originalTags,withTags);
	}
	method checkUniqueTagsHelper(originalTags,withTags){
		
		/*
		output.writeLine("-----------------------------------");
		originalTags.map(fn (c) => {
			output.writeLine(c);
		});	
		withTags.map(fn (c) => {
			output.writeLine(c);
		});	
		output.writeLine("-----------------------------------");
		*/
		
		originalTags.map(fn (c) => {
			withTags.map(fn (d) => {
				if (c==d && !(c=="")){
					output.writeLine("Error: Attempt to compose state from two states containing tag with same name"+c);
				};
			});
		});	
	}
		
	method specializeState(allMembers,withMembers,memberToAdd,memberValue,withEntry,targetStateName){
		var foundMatch=false;
		withMembers.map(fn (c) => {
			if (c==memberToAdd){
				foundMatch=true;
				output.writeLine(targetStateName+"['"+memberToAdd+"'] = '"+memberValue+"';");
			};
		});	
		if (foundMatch==false) {
			allMembers.map(fn (c) => {
				if (c==memberToAdd){
				output.writeLine("Error: Attempt to compose state from two states containing member with same name");
				};
			});	
			output.writeLine(generateNewStateName(targetStateName)+"['"+memberToAdd+"'] = "+memberValue+";");
			//must add the member to the tree now
			withEntry.node.memberList.add("'"+memberToAdd+"'");
		};
	}
	
	method handleAddWithState(qiToAdd,parentNode,parentTree){
		match (qiToAdd) {	
			case QI {			
				//the string name of the state that is a with state for the current state
				var withState = qiToAdd.qid.toString();	
				//the object that bundles the node and tree corresponding to the state name above
				var withEntry = this.getState(withState);
				var withRoot = withEntry.tree.root;
				var tagStateName = generateNewStateName(parentNode.tag);
				var withTagStateName = generateNewStateName(withEntry.name);
				
				//ensure the members being added by the with state are not already present in the state to which they are added
				//checkUniqueMembers(parentTree.root,withRoot);
		
				//ensure the tags being added by the with state are not already present in the state to which they are added
				checkUniqueTags(parentTree.root,withRoot);
		
				withRoot.children.map(fn (c) => {
					parentNode.children.add(c);
				});
				
				var withMembersList = withRoot.members();
				withMembersList.map(fn (d) => {
					output.writeLine(tagStateName+"["+d+"] = "+withTagStateName+"["+d+"];");
				});
				
				match (qiToAdd.initState) {
					case Some{
						match (qiToAdd.initState.value) {
							case DeclList{
								var withMembers = withEntry.node.members();
								var allMembers = withRoot.members();
								qiToAdd.initState.value.decls.map(fn (d) => {
									var funcName = "";
									match (d){
										case FieldDecl{
											funcName=d.f.name;
										}
										case MethodDecl{
											funcName=d.name;
										}
										default{
											output.writeLine("problem in specialize");
										}
									};
									var funcValue = d.accept(this);
									//check unique members
									specializeState(allMembers, withMembers, funcName, funcValue, withEntry, parentNode.tag);
								});
							}
							default{
								output.writeLine("//problem in QI in StateDecl in handleAddWith");
							}
						};
					}
					case None{
					}
				};
				
			}
			
			default{
				output.writeLine("problem");
			}
		};
		
	}

	method visitApplication(node) {
		if (debug) {
			output.writeLine("//I am an Application");
			output.writeLine ("   //f: " + node.f.toString());
			output.writeLine ("   //arguments: " + node.arguments.toString());
			output.writeLine ("   //isTailCall: " + node.isTailCall.toString());
		};
		
		//make a string (with argument values) from list of argument objects	
		var argumentsString="";
		if (node.arguments.length() > 0) {
			var nMinusOneArguments = node.arguments.subList(0,node.arguments.length() - 2);
			nMinusOneArguments.map(fn (a) => argumentsString=argumentsString+a.accept(this)+",");
			argumentsString=argumentsString+(node.arguments.get(node.arguments.length() - 1).accept(this));
		};
		
		
		//is there a problem here?  where a name cannot be found?
		var mn = node.f.name;
		
		//------------------------------------------------------------
		//this is temporary while we figure out packages.  fix this later!
		var extraArgumentToRemoveLater = "";	
		if (mn=="printLine"){
			extraArgumentToRemoveLater = "+\"<BR>\"";
		};
		if (mn=="printLine" || mn=="print"){
			mn="document.write";
		};	
		//---------------------------------------------------------------
		
		//immediately emit code that assigns method call's value to the newVarName generated below
		
		var newVarName = generateNewVariableName();
		var funcCallString = "var "+newVarName+" = "+mn+"("+argumentsString+extraArgumentToRemoveLater+");";
		//print the function call
		output.writeLine (funcCallString);
		//return the newVarName so that method call's value can be used by the method that called this one (this can be used just like an ID name)
		newVarName;
	}
	method visitAssignment(node) {
		if (debug) {
			output.writeLine("//I am an Assignment");
			output.writeLine ("   //target: " + node.target.toString());
			output.writeLine ("   //field: " + node.field.toString());
			output.writeLine ("   //value: " + node.value.toString());
		};
		var fieldName = node.field.accept(this);
		var fullName = "";
		match (node.target) {
			case None{
				fullName = fieldName;
			}
			case Some{
				fullName = node.target.value.accept(this)+"."+fieldName;
			}
		};
		output.writeLine (fullName+" = "+node.value.accept(this)+";");
		//node.target.value.accept(this); 
		fullName;
	}
	method visitAtomicBlock(node) {
		if (debug) {
			output.writeLine("//I am an AtomicBlock");
		};
	}
	method visitBorrowBinding(node) {
		if (debug) {
			output.writeLine("//I am a BorrowBinding");
		};
		node.exp.accept(this); 
		node.body.accept(this); 
	}
	method visitCase(node) {
		if (debug) {
			output.writeLine("//I am a Case");
			output.writeLine ("   //qi: " + node.qi.toString());
			output.writeLine ("   //x: " + node.x.toString());
			output.writeLine ("   //e: " + node.e.toString());
			output.writeLine ("   //defaultCase: " + node.defaultCase.toString());
			output.writeLine ("   //boundVar: " + node.boundVar.toString());
		};
		//the one called below should only be called if this case is not the default case
		
		ifElse(this.firstCase){
			output.writeLine("if (matchVar.match(\""+node.qi.value.qid.toString()+"\")) {"); 
			this.firstCase=false;	
		}
		{
			ifElse(node.defaultCase){
				output.writeLine("else {");
			}
			{
				output.writeLine("else if (matchVar.match(\""+node.qi.value.qid.toString()+"\")) {"); 
			};
		};
		node.e.accept(this); 
		output.writeLine("}");
		"matchVar";
	}
	method visitCast(node) {
		if (debug) {
			output.writeLine("//I am a Cast");
		};
		node.body.accept(this);
	}
	method visitChangeState(node) {
		if (debug) {
			output.writeLine("//I am a ChangeState");
			output.writeLine ("   //e: " + node.e.toString());
			output.writeLine ("   //s: " + node.s.toString());
		};
		var toChange=node.e.accept(this);
		
		match(node.s){
			case QI{
				output.writeLine(toChange+".stateChange("+node.s.accept(this)+");");
			}
			case With{
				output.writeLine(toChange+".stateChange("+node.s.accept(this)+");");
			}
			case DeclList{
				node.s.decls.map(fn (d) => {
					match(d) {
						case MethodDecl{
							//java.lang.System.out.print(toChange+"['"+d.name+"'] = "); 
							//d.accept(this);
							output.writeLine(toChange+".stateChange("+toChange+".withMember('"+d.name+"', '"+d.accept(this)+"'));");
							}
						case FieldDecl{			
							match(d.e){
								case UnitLiteral{
									//output.writeLine(toChange+".stateChange("+toChange+".withMemberNoValue('"+d.f.name+"'));");
									output.writeLine(toChange+".stateChangeMemberNoValue('"+d.f.name+"');");
								}
								default {
									//output.writeLine(toChange+".stateChange("+toChange+".withMember('"+d.f.name+"', '"+d.e.accept(this)+"'));");
									output.writeLine(toChange+".stateChangeMember('"+d.f.name+"', '"+d.e.accept(this)+"');");
								}
							};
						
						}
						default{
							d.accept(this);
						}
					};
				});
			}
			default{
				output.writeLine("problem at changeState's s");
				node.s.accept(this);
			}
		};
		toChange;
	}
	method visitCompilationUnit(node) {
		if (debug) {
			output.writeLine("//I am a CompilationUnit");	
		};
		
		var directoryFile = new plaid.io.FileObj;
		directoryFile.setName(targetDirectory);
		var modifiedPackage="";
		
		ifElse(directoryFile.canWrite()){
			node.packageName.map(fn (d) => {
				modifiedPackage=modifiedPackage+"/"+d;
			});
			directoryFile.setName(targetDirectory+modifiedPackage);
			directoryFile.mkdirs();
		}
		{
			output.writeLine("Error: cannot write to the file "+targetDirectory+modifiedPackage);
		};
		
		var fileName=node.sourceFile;
		java.lang.System.out.println(fileName);	
		var fileExtension=".js";
		if(browser==true && mainFile==true){
			fileExtension=".html";
		};
		output.setName(targetDirectory+modifiedPackage+"/"+fileName+fileExtension);
		output.beginWrite();
		
		ifElse (browser==true) {
		    if(mainFile==true){
				output.writeLine("<html>");
				output.writeLine("<body>");
				output.writeLine("<script src=\"prototype.js\" type=\"text/javascript\"></script>");
				output.writeLine("<script src=\"Tree.js\" type=\"text/javascript\"></script>");
				output.writeLine("<script src=\"PlaidObject.js\" type=\"text/javascript\"></script>");
				output.writeLine("<script src=\"PlaidState.js\" type=\"text/javascript\"></script>");
				output.writeLine("<script type=\"text/javascript\">");
				output.writeLine("<!--");
			
				output.writeLine("var Script = {");
				output.writeLine("loadedScripts: [],");
				output.writeLine("has: function(array, item) {");
				output.writeLine("var length=array.length;");
				output.writeLine("for (var i=0;i<length;i++){");
				output.writeLine("if (array[i]===item){");
				output.writeLine("return true;");
				output.writeLine("}");
				output.writeLine("}");
				output.writeLine("return false;");
				output.writeLine("},");
				output.writeLine("include: function(script){");
				output.writeLine("if (Script.has(this.loadedScripts,script)){");
				output.writeLine("return false;");
				output.writeLine("}");
				output.writeLine("var code = new Ajax.Request(script, {");
				output.writeLine("asynchronous: false, method: \"GET\",");
				output.writeLine("evalJS: false, evalJSON: false");
				output.writeLine("}).transport.responseText;");
				output.writeLine("if (Prototype.Browser.IE) {");
				output.writeLine("window.execScript(code);");
				output.writeLine("} else if (Prototype.Browser.WebKit){");
				output.writeLine("$$(\"head\").first().insert(Object.extend(");
				output.writeLine("new Element(\"script\", {type: \"text/javascript\"}), {text: code}");
				output.writeLine("));");
				output.writeLine("} else {");
				output.writeLine("window.eval((code));");
				output.writeLine("}");
				output.writeLine("this.loadedScripts.push(script);");
				output.writeLine("}");
				output.writeLine("};");
			};
		}
		{
			output.writeLine("load(\"PlaidObject.js\");");
			output.writeLine("load(\"PlaidState.js\");");
			output.writeLine("load(\"Tree.js\");");
		};
		node.imports.accept(this);
		node.decls.map(fn (d) => {
			d.accept(this);
		});
		if (browser==true && mainFile==true){
			output.writeLine("//-->");
			output.writeLine("</script>");
			output.writeLine("</body>");
			output.writeLine("</html>");
		};
		output.endWrite();
	}
	method visitDeclList(node) {
		if (debug) {
			output.writeLine("//I am a DeclList");
		};
		//node.decls.map(fn (d) => {d.accept(this);});
		
		var nMinusOneDecls = node.decls.subList(0,node.decls.length() - 2);
		nMinusOneDecls.map(fn (d) => {d.accept(this);});
		node.decls.get(node.decls.length() - 1).accept(this);
	}
