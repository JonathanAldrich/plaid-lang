	method visitLambda(node) {
	
		if (debug) {
			output.writeLine("//I am a Lambda");
			output.writeLine ("   //arguments: " + node.arguments.toString());
			output.writeLine ("   //body: " + node.body.toString());
			output.writeLine ("   //lambdaType: " + node.lambdaType.toString());
			output.writeLine ("   //recursiveName: " + node.recursiveName.toString());
		};
		//make a strings (with argument values) from list of argument objects	
		var newVarName = generateNewVariableName();
		var argumentsString="";
		if (node.arguments.length() > 0) {
			var nMinusOneArguments = node.arguments.subList(0,node.arguments.length() - 2);
			nMinusOneArguments.map(fn (a) => argumentsString=argumentsString+a.accept(this)+",");
			argumentsString=argumentsString+(node.arguments.get(node.arguments.length() - 1).accept(this));
		};
		output.writeLine ("var "+newVarName+" = function("+argumentsString+") {");
		output.writeLine ("return "+node.body.accept(this)+";"); 
		output.writeLine ("};");
		newVarName;
	}
	method visitLetBinding(node) {
		if (debug) {
			output.writeLine("//I am a LetBinding");
			output.writeLine ("   //x: " + node.x.toString());
			output.writeLine ("   //exp: " + node.exp.toString());
			output.writeLine ("   //body: " + node.body.toString());
			output.writeLine ("   //mutable: " + node.mutable.toString());
		};
		output.writeLine("var "+node.x.accept(this)+" = "+node.exp.accept(this)+";"); 
		node.body.accept(this); 
	}
	method visitMatch(node) {
		if (debug) {
			output.writeLine("//I am a Match");
			output.writeLine ("   //e: " + node.e.toString());
			output.writeLine ("   //caseList: " + node.caseList.toString());
		};
		output.writeLine("var matchVar = "+node.e.accept(this)+";");
		this.firstCase = true;
		node.caseList.map(fn (c) => {
			c.accept(this);
		});
		node.e.accept(this);
	}
