package plaid.aeminium.codegen;

import plaid.ast.visitor.ASTViewer;

method main() {
	val filename = "testInputs/parallelCalls.json";
	val reader = java.io.BufferedReader.new(java.io.FileReader.new(filename));
	val jsonInput = reader.readLine();
	reader.close();
	
	// Load permtype annotated AST from json file
    val plaidAST = plaid.lang.Serialization.fromJSON(jsonInput);
	plaidAST.accept(new ASTViewer);
	
	// Generate aeminium enabled code
	// plaidAST.accept(new AeminiumCodeGen);
}